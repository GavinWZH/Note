def func_time(func):
    def inner(*args, **kw):
        import time
        start_time = time.time()
        result = func(*args, **kw)
        end_time = time.time()
        print('函数%s运行时间为：%s秒' % (func.__name__, round(end_time-start_time, 4)))
        return result
    return inner



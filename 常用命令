SELECT
    at.id AS trade_id,
    ssh.id AS staff_history_id,
    ssch.id AS staff_category_histroy_id
FROM account_transaction AS at
JOIN staff_staffhistory AS ssh
ON
        at.trader_staff_id = ssh.staff_id
    AND
        at.time >= ssh.start_date
    AND
        (
                ssh.end_date is NULL
            OR
                DATE_FORMAT(at.time, '%Y-%m-01 00:00:00.000000') <= ssh.end_date
        )
JOIN staff_staffcategoryhistory AS ssch
ON
        at.trader_staff_id = ssch.staff_id
    AND
        at.time >= ssch.start_date
    AND
        (
                ssch.end_date is NULL
            OR
                DATE_FORMAT(at.time, '%Y-%m-01 00:00:00.000000') <= ssch.end_date
        )
WHERE
    at.trader_staff_id=214
ORDER BY at.id;


